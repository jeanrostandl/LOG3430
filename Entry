public class Entry<K, V> {	
        K key;
        V value;
        Entry<K,V> next;
    
        public Entry(K key, V value, Entry<K,V> next){
            this.key = key;
            this.value = value;
            this.next = next;
        }
        
        public V getValue() {
            return value;
        }
     
        public void setValue(V value) {
            this.value = value;
        }
     
        public K getKey() {
            return key;
        }
    }
